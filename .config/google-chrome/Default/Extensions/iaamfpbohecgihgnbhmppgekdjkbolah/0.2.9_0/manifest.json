{
   "app": {
      "background": {
         "scripts": [ "app/vendor/jquery-2.1.3.min.js", "app/vendor/underscore.js", "app/vendor/backbone.js", "app/background.js" ]
      }
   },
   "author": "FireRTC Team",
   "description": "The most convenient way to make phone calls while working or studying.\nFree phone calls to the US and Canada. No nonsense.",
   "icons": {
      "128": "assets/images/firertc-icon-128.png",
      "32": "assets/images/firertc-icon-32.png",
      "48": "assets/images/firertc-icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "FireRTC",
   "permissions": [ "*://*.firertc.com/*", "https://gravatar.com/avatar/*", "videoCapture", "audioCapture", "webview", "notifications", "identity", "storage", "unlimitedStorage", "browser" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "url_handlers": {
      "firertc_phone": {
         "matches": [ "https://phone.firertc.com/phone*" ],
         "title": "Launch FireRTC Phone"
      }
   },
   "version": "0.2.9"
}
